root = true

[**]
charset = utf-8
indent_style = space
indent_size = 4
tab_width = 4
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true
max_line_length = 70

# Ignore paths
[{**/node_modules/**/*,**/dist/**/*,**/build/**/*,**/release/**/*}]
ij_formatter_enabled = false
generated_code = true
